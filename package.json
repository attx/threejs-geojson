{
  "name": "@attx/threejs-geojson",
  "version": "1.0.7",
  "description": "Create threejs geometries from geojson",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib/*",
    "src/*",
    "dist/*"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:example",
    "build:lib": "tsc --project tsconfig.lib.json",
    "build:example": "vue-tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore",
    "test:unit": "echo \"no test specified\""
  },
  "keywords": [
    "threejs",
    "geojson"
  ],
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.7",
    "@types/three": "^0.146.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "naive-ui": "^2.33.5",
    "sass": "^1.55.0",
    "three": "^0.146.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.1",
    "vue-tsc": "^1.0.9"
  }
}
